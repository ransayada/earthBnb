    "description": "```\r npm install\r ```",
    "main": "babel.config.js",
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ransayada/earthBnb.git"
    },
    "keywords": [],
